{
  "manifest_version": 2,
  "name": "YT Distraction Killer",
  "description": "Removing youtube features for focused work",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": ["content.js"]
  
    }
  ],
  "browser_action": {
    "default_popup": "./index.html",
    "default_title": "click to configure",
    "default_icon" :  "icon.png"
  },  
  "background": {
    "scripts": ["background.js"]
  }
}

